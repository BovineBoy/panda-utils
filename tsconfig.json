{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "strict": true,
    "outDir": "./dist/",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
